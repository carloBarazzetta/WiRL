        <RuntimeOnlyPackage>true</RuntimeOnlyPackage>
        <DCC_DcuOutput>..\..\Lib\Delphi10.3\$(Platform)\$(Config)</DCC_DcuOutput>
        <DllSuffix>260</DllSuffix>
